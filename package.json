{
  "name": "lcov-badge2",
  "version": "1.0.0",
  "description": "Generate a badge from a LCOV report",
  "keywords": [
    "lcov",
    "badge",
    "coverage",
    "test"
  ],
  "bin": "./lcov-badge",
  "main": "main.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevenhair/lcov-badge2.git"
  },
  "author": "Steve Hair <me@stevenhair.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stevenhair/lcov-badge2/issues"
  },
  "homepage": "https://github.com/stevenhair/lcov-badge2#readme",
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "coverage": "jest --coverage",
    "postbuild": "gulp postbuild",
    "prebuild": "gulp clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts"
  },
  "dependencies": {
    "badge-up": "^3.0.0",
    "minimist": "^1.2.5",
    "minimist-options": "^4.1.0",
    "parse-lcov": "^1.0.4"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/minimist": "^1.2.1",
    "@types/minimist-options": "^3.0.0",
    "@types/node": "^15.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "del": "^6.0.0",
    "eslint": "^7.25.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-unicorn": "^31.0.0",
    "gulp": "^4.0.2",
    "jest": "^26.6.3",
    "pre-commit": "^1.2.2",
    "ts-jest": "^26.5.5",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": ">=14"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "modulePaths": [
      "<rootDir>"
    ],
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.ts"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "pre-commit": [
    "lint",
    "test"
  ]
}
